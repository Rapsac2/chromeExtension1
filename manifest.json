{
    "manifest_version": 2,
    "name": "Youtube Time Management",
    "version": "0.1",
    "content_scripts": [
        {
            "matches":[
                "http://www.youtube.com/*",
                "https://www.youtube.com/*"
            ],
            "js": ["mainTab.js"]
        }
    ],
    "background": {
        "scripts": ["mainBackground.js"]
    }
}